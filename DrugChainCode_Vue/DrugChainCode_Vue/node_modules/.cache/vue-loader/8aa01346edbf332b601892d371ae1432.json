\"nextPage()\">\r\n      <img src=\"@/img/翻页2.svg\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  components: {\r\n    axios\r\n  },\r\n  data() {\r\n    return {\r\n      title: '药品管理追踪溯源系统——药物仓库',\r\n      // explanation: '注：当药物生产后，每一件都会自动生成独一无二的身份码，用于防伪，溯源。且不需要手动入库，会自动加入库存中。',\r\n      UserInformation: '',\r\n      warehouseDateJSON: '',\r\n      Page: 0\r\n    }\r\n  },\r\n  created() {\r\n    this.getUser()\r\n  },\r\n  mounted() {\r\n    this.getwarehouseDates()\r\n  },\r\n  methods: {\r\n    //获取用户信息\r\n    getUser() {\r\n      var UserInformation = JSON.parse(sessionStorage.getItem('UserInformation'))\r\n      if (UserInformation != null) {\r\n        this.UserInformation = UserInformation\r\n      } else {\r\n        alert('网络异常，请重试')\r\n        this.$router.push('/Login')\r\n      }\r\n    },\r\n\r\n    //获取仓库中的数据\r\n    getwarehouseDates() {\r\n      axios\r\n        .get('http://localhost:8002/getWarehouseInformation', {\r\n          params: {\r\n            OwnerID: this.UserInformation.UID\r\n          }\r\n        })\r\n        .then(res => {\r\n          if (res.data.Code != 0) {\r\n            this.warehouseDateJSON = res.data.data\r\n          } else {\r\n            alert(res.data.data)\r\n          }\r\n        })\r\n    },\r\n\r\n    //上一页\r\n    previousPage() {\r\n      if (this.Page > 0) {\r\n        this.Page--\r\n      }\r\n    },\r\n    //下一页\r\n    nextPage() {\r\n      if (this.Page < this.warehouseDateJSON.length - 1) {\r\n        this.Page++\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#bottom {\r\n  width: 100%;\r\n  position: absolute;\r\n  top: 75px;\r\n  bottom: 0px;\r\n  left: 0px;\r\n}\r\n\r\n#bottom .MainTitle {\r\n  margin: 0 auto;\r\n  margin-top: 50px;\r\n  width: 100%;\r\n  font-size: 50px;\r\n  /* 设置字符间距 */\r\n  letter-spacing: 5px;\r\n  text-align: center;\r\n}\r\n\r\n#previousPage,\r\n#nextPage {\r\n  position: absolute;\r\n  top: